{
   "repositories":[
      {
         "type":"composer",
         "url":"https://wpackagist.org"
      },
      {
         "type": "composer",
         "url": "https://packages.metabox.io/YOUR_API_KEY"
      }
   ],
   "require":{
      "wpackagist-plugin/meta-box":"dev-trunk",
      "meta-box/meta-box-aio": "dev-master"
   },
   "extra":{
      "installer-paths":{
         "vendor/meta-box/meta-box":[
            "wpackagist-plugin/meta-box"
         ],
         "vendor/meta-box/meta-box-aio":[
            "meta-box/meta-box-aio"
         ]
      }
   },
   "autoload":{
      "files":[
         "vendor/meta-box/meta-box/meta-box.php",
         "vendor/meta-box/meta-box-aio/meta-box-aio.php"
      ]
   }
}
